@model IEnumerable<ASP.NETCoreEmptyProject.Models.PeoplLanguagesModel>

@{
    ViewData["Title"] = "People";
}
<div class="text-center">
    <h3>Languages and People</h3>
</div>
<div class="container">

    <div class="container">
        <table class="table">
            <thead>
                <tr>
                    <th>Person ID</th>
                    <th>Language</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {

                    <tr>
                        <td>@item.PersonId</td>
                        <td>@item.Language</td>
                    </tr>
                }

            </tbody>
        </table>

    </div>
</div>
