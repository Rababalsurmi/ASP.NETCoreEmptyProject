@model List<ASP.NETCoreEmptyProject.Models.CountryModel>

@{
    ViewData["Title"] = "People";
}
<div class="text-center">
    <h3>Countries and Cities</h3>
</div>
<div class="container">

    <div class="container">
        <table class="table">
            <thead>
                <tr>
                    <th>Country</th>
                    <th>City</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                 {

                   <tr>
                    <td>@item.CountryName</td>

                       <td>@foreach (var c in item.Cities)
                            {
                               @c.CityName
                            }
                       </td>

                    
                   </tr>
                 }

            </tbody>
        </table>

    </div>
</div>
