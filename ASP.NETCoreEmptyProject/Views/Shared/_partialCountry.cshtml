@model List<ASP.NETCoreEmptyProject.Models.CountryCityModel>


<div class="container">
    <h2>List of Cities</h2>

    <table class="table">
        <thead>
            <tr>
                <th>Country</th>
                <th>City</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var country in Model)
            {
                <tr>
                    <td>@country.country</td>
                    <td>@country.city</td>
                </tr>
            }
        </tbody>

    </table>
</div>